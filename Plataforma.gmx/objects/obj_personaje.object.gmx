            <string>/// movimiento del personaje
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_jump = keyboard_check_pressed(vk_space);
key_jump_held = keyboard_check(vk_space);
key_down = keyboard_check(vk_down);

move = key_left + key_right;
hsp = move* movespeed;
if(vsp &lt; 10) vsp += grav;

if(place_meeting(x,y + 1, obj_Muro))
{
  jumps = jumpsmax;
}
if(key_jump) &amp;&amp; (jumps &gt; 0)
{
jumps -= 1;
if(key_jump)vsp = -jumpspeed;
}
if(key_jump) &amp;&amp; (place_meeting(x+1, y, obj_Muro) || place_meeting(x-1, y, obj_Muro))
{
      vsp = -jumpspeed;
}
if(vsp &lt;0) &amp;&amp; (!key_jump_held) vsp = max(vsp , -jumpspeed/2)

var hsp_final = hsp + hsp_carry
hsp_carry = 0;

// colision horizontal
if(place_meeting(x + hsp_final, y, obj_Muro))
{
while(!place_meeting(x + sign(hsp_final), y, obj_Muro))
{
   x += sign(hsp_final);
  }
  hsp_final = 0;
  hsp = 0;
}
x += hsp_final;

//colision vertical
if(place_meeting(x,y + vsp, obj_Muro))
{
while(!place_meeting(x, y + sign(vsp), obj_Muro))
{
   y += sign(vsp);
  }
  vsp = 0;
}
y += vsp;


